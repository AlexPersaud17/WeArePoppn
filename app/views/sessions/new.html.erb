<h2>Login</h2>

<%= render :partial => "./errors"%>

<%= form_tag sessions_path do  %>
  <p>
    <%= label_tag "Email"%>
    <%= text_field_tag "email" %>
  </p>
  <p>
    <%= label_tag "Password" %>
    <%= password_field_tag "password" %>
  </p>
  <p>
    <%= submit_tag "Login" %>
  </p>

<%end%>
